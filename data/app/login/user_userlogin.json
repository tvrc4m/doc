{
  "desc": "检查验证码。此接口只做检查，无论结果如何都不会销毁验证码，因此可以在其他需要验证码的接口重复使用验证码",
  "params": {
    "code": {
      "desc": "验证码",
      "must": "是",
      "type": "string"
    },
    "phone": {
      "desc": "手机号",
      "must": "是",
      "type": "string"
    },
    "sessionid": {
      "desc": "发送短信验证码接口返回的sessionid  ",
      "must": "是",
      "type": "string"
    }
  },
  "return": {
    "create_time": {
      "desc": "创建时间",
      "must": "是",
      "type": "string"
    },
    "gender": {
      "desc": "用户性别",
      "must": "是",
      "type": "string"
    },
    "id": {
      "desc": "用户id",
      "must": "是",
      "type": "int"
    },
    "image": {
      "desc": "大头像",
      "must": "是",
      "type": "string"
    },
    "nickname": {
      "desc": "用户昵称",
      "must": "是",
      "type": "string"
    },
    "sessionid": {
      "desc": "sessionid",
      "must": "是",
      "type": "string"
    },
    "weibo.status": {
      "desc": "微博",
      "must": "是",
      "type": "boolean true or false"
    },
    "weibo.openid": {
      "desc": "openid",
      "must": "否",
      "type": "string"
    },
    "weixin.status": {
      "desc": "微信",
      "must": "是",
      "type": "boolean true or false"
    },
    "weixin.openid": {
      "desc": "openid",
      "must": "否",
      "type": "string"
    },
    "qq.status": {
      "desc": "qq",
      "must": "是",
      "type": "boolean true or false"
    },
    "qq.openid": {
      "desc": "openid",
      "must": "否",
      "type": "string"
    }
  },
  "title": "手机号码登录",
  "type": "post",
  "url": "appapi/user/userlogin",
  "version": "v1.0.1"
}