      "must": "否",
      "type": "int"
    },
    "page": {
      "desc": "页码",
      "must": "否",
      "type": "int"
    }
  },
  "return": {
    "datas": {
      "desc": "需求数据",
      "must": "是",
      "type": "array"
    },
    "datas.title": {
      "desc": "大标题日期",
      "must": "是",
      "type": "array"
    },
    "datas.content.id": {
      "desc": "直播id",
      "must": "是",
      "type": "int"
    },
    "datas.content.title": {
      "desc": "直播标题",
      "must": "是",
      "type": "string"
    },
    "datas.content.subtitle": {
      "desc": "副标题",
      "must": "是",
      "type": "string"
    },
    "datas.content.court_name": {
      "desc": "法院名称",
      "must": "是",
      "type": "string"
    },
    "datas.content.start_time": {
      "desc": "开播时间",
      "must": "是",
      "type": "string"
    },
    "datas.content.flash_obj": {
      "desc": "flash地址",
      "must": "是",
      "type": "string"
    },
    "datas.content.jump_type": {
      "desc": "跳珠类型 1为app 2为h5",
      "must": "是",
      "type": "int"
    },
    "datas.content.startTime": {
      "desc": "距离开始的具体时间段",
      "must": "是",
      "type": "string"
    }
