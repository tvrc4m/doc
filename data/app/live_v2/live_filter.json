{
  "desc": "直播列表及筛选条件",
  "params": {
    "case_type": {
      "desc": "筛选条件--案件类型",
      "must": "否",
      "type": "int"
    },
    "court": {
      "desc": "筛选条件--某个法院",
      "must": "否",
      "type": "int"
    },
    "court_level": {
      "desc": "筛选条件--法院级别",
      "must": "否",
      "type": "int"
    },
    "province_id": {
      "desc": "筛选条件--省id",
      "must": "否",
      "type": "int"
    },
    "stat": {
      "desc": "筛选条件--直播状态",
      "must": "否",
      "type": "int"
    },
    "type": {
      "desc": "筛选条件--直播类型",
      "must": "否",
      "type": "int"
    },
    "live_type_id": {
      "desc": "直播类型,代表是每个类型下面的筛选。0代表是直播大厅筛选",
      "must": "是",
      "type": "int"
    },
    "page": {
      "desc": "页数,默认为1",
      "must": "否",
      "type": "int"
    },
    "pcount": {
      "desc": "每页获取的数量,默认为20",
      "must": "否",
      "type": "int"
    }
  },
  "return": {
    "stat": {
      "desc": "筛选---直播状态",
      "must": "是",
      "type": "array"
    },
    "type": {
      "desc": "筛选---类型(只存在与直播大厅 即:live_type_id=0)",
      "must": "是",
      "type": "array"
    },
    "case_type": {
      "desc": "筛选---案件类型(只存在与庭审大厅 即:live_type_id=1)",
      "must": "是",
      "type": "array"
    },
    "court_level": {
      "desc": "筛选---法院级别(只存在与庭审大厅 即:live_type_id=1)",
      "must": "是",
      "type": "array"
    },
    "court": {
      "desc": "筛选---法院名称(只存在与庭审大厅 即:live_type_id=1)",
      "must": "是",
      "type": "array"
    },
    "data": {
      "desc": "直播数组对应的key",
      "must": "是",
      "type": "array"
    },
    "data.cover_image": {
      "desc": "直播封面图片",
      "must": "是",
      "type": "string"
    },
    "data.id": {
      "desc": "直播id",
      "must": "是",
      "type": "int"
    },
    "data.stat": {
      "desc": "直播状态",
      "must": "是",
      "type": "int"
    },
    "data.jump_type": {
      "desc": "跳转类型页面，<code><a href='#live_02'>点击查看详情</a></code>",
      "must": "是",
      "type": "int"
    },
    "data.subtitle": {
      "desc": "副标题。庭审直播时取的是法院名字，可为空",
      "must": "否",
      "type": "int"
    },
    "data.url": {
      "desc": "直播详情页的链接",
      "must": "是",
      "type": "string"
    }
  },
  "title": "直播列表及筛选",
  "type": "post",
  "url": "appapi/live/getfilter",
  "version": "v2.1.0"
}