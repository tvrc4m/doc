{
  "title": "直播获取地址",
  "desc": "经纬度都经过header传值(Lng:经度;Lat:纬度)",
  "type": "post",
  "version":"v2.0.6新增",
  "url": "appapi/aliyunbroadcast/getBroadcastAddress",
  "params": {
    "Lng": {
      "desc": "经度-- 通过header传值",
      "must": "是",
      "type": "string",
      "version":"v2.0.6"
    },
    "Lat": {
      "desc": "纬度-- 通过header传值",
      "must": "是",
      "type": "string",
      "version":"v2.0.6"
    }
  },
  "return": {
    "address": {
      "desc": "地址",
      "type": "string"
    }
  }
}