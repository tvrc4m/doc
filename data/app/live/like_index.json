{
  "title": "视频,VR，直播点赞接口",
  "desc": "未登录情况下，以ip作为用户的唯一判断条件，在已登录的情况下,会在header中传递sessionid,以用户id作为判断条件",
  "type": "post",
  "version":"v2.0.6新增",
  "url": "appapi/like/index",
  "params": {
    "type": {
      "desc": "点赞类型,4:代表视频 11:代表VR 9:直播",
      "must": "是",
      "type": "int",
      "version":"v2.0.6"
    },
    "content_id": {
      "desc": "内容id",
      "must": "是",
      "type": "int",
      "version":"v2.0.6"
    },
    "is_like": {
      "desc": "1:代表点赞 0:代表取消点赞",
      "must": "是",
      "type": "int",
      "version":"v2.0.6"
    },
    "sessionid": {
      "desc": " 用户sessionid",
      "must": "否",
      "type": "int",
      "version":"v2.0.6"
    }
  }
}