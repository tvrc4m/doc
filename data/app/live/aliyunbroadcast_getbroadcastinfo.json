{
  "desc": "客户端发起直播",
  "params": {
    "title": {
      "desc": "直播的主题或标题",
      "must": "是",
      "type": "string",
      "version":"v2.0.6"
    },
    "address": {
      "desc": "定位的地址或用户输入的地址",
      "must": "是",
      "type": "string",
      "version":"v2.0.6"
    },
    "phone": {
      "desc": "用户的手机号",
      "must": "是",
      "type": "string",
      "version":"v2.0.6"
    },
    "sessionid": {
      "desc": "用户sessionid",
      "must": "是",
      "type": "string",
      "version":"v2.0.6"
    }
  },
  "return": {
    "cid": {
      "desc": "直播内容id",
      "type": "int"
